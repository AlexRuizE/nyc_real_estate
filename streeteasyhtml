    <script type="text/javascript">
  var seUserTimingMarkMs;
  window.clientProfiler = {
    timingCategory: function (mark) {
      return {
        'search.rentals': 'RentalSearch',
        'search.sales': 'SaleSearch',
        'search.buildings': 'BuildingSearch',
        'home.rentals': 'RentalsHome',
        'home.sales': 'SalesHome',
        'home.buildings': 'BuildingsHome',
        'rental': 'RentalHdp',
        'sale': 'SaleHdp',
        'building': 'BuildingHdp',
        'property': 'PropertyHdp',
        'complex': 'ComplexHdp',
        'closing': 'ClosingHdp'
      }[mark.split('.').slice(0, -1).join('.').toLowerCase()]
    },

    timingLabel: function (mark) {
      return {
        'listingImageVisible': 'ListingThumb',
        'locationsPopulated': 'Locations',
        'searchButtonVisible': 'SearchButton',
        'mainImageVisible': 'ListingImage'
      }[mark.split('.').pop()]
    },

    setMark: function (name, prefix) {
      if (typeof performance !== 'undefined' && 'mark' in performance && name && seUserTimingMarkMs == null) {
        var normalizedPrefix = prefix || '';
        var prefixedMarkName = normalizedPrefix + name;

        performance.clearMarks(prefixedMarkName);
        performance.mark(prefixedMarkName);
        seUserTimingMarkMs = Math.round(performance.now());

        if (typeof RProfiler === 'object') {
          RProfiler.addInfo('indicator', 'i33394', seUserTimingMarkMs);
        }

        if (typeof ga !== 'dataLayer') {
          dataLayer.push({
            'event': 'timingMark',
            'timingVar': prefixedMarkName,
            'timingCategory': clientProfiler.timingCategory(prefixedMarkName),
            'timingLabel': clientProfiler.timingLabel(prefixedMarkName),
            'timingValue': Math.round(performance.now())
          });
        }
        if (typeof newrelic === 'object') {
          newrelic.addPageAction(prefixedMarkName);
        }
      }
    }
  };
</script>

