    <script type="text/javascript">
  WebFontConfig =
  {
    google: { families: ["Source Sans Pro:300,400,600,700"] },
    active: function () { performance.mark('fontsVisible'); }
  };
  (function() {
    var wf = document.createElement('script');
    wf.src = 'https://ajax.googleapis.com/ajax/libs/webfont/1.6.16/webfont.js';
    wf.type = 'text/javascript';
    wf.async = 'true';
    var s = document.getElementsByTagName('script')[0];
    s.parentNode.insertBefore(wf, s);
  })();
</script>

    <!-- Impressions Tracking Config -->
<script type="text/javascript">
  var SE_Tracker = (function(host){
    var now = new Date().getTime();
    var cache = [];

    function inCache(key){
      for(var i in cache){
        if(cache[i] == key){
          return true;
        }
      }
      return false;
    }

    function post(url, params, callback){
      if(inCache(params.key)){
        return false;
      }

      cache.push(params.key);

      var request = new XMLHttpRequest();

      request.withCredentials = true;
      request.open('POST', url, true);
      request.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded; charset=UTF-8');

      request.onload = callback;

      request.onerror = function(xhr, status, err) {
        console.error(xhr, status, err.toString());
      };

      var query_string = [];

      for(var key in params){
        query_string.push(key + '=' + encodeURIComponent(params[key]));
      }

      request.send(query_string.join('&'));
    }
    return {
      push: function(key){
        if(key){
          new Image().src = ['//', host, '/in/', key, '?r=', now].join('');
        }
      },
      track: function(params){
        post('/api/v4/events/track', params);
      },
      durablePush: function(key) {
        var deferred = $j.Deferred();

        if(key) {
          var image = new Image();
          var resolve = function() {
            deferred.resolve();
          };

          image.onload = resolve;
          image.onerror = resolve;
          image.src = ['//', host, '/in/', key, '?r=', now].join('');
        } else {
          deferred.resolve();
        }

        return deferred.promise();
      },
      cache_keys: cache
    };
  })('tracker-legacy.streeteasy.com');
</script>
