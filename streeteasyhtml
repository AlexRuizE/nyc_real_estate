      <script src="//cdn-assets-s3.streeteasy.com/assets/app-aa8d0da0f94e6e034726cb85dfd193e6df689a9e07c13cca6f4696e22a16ce64.js"></script>
<script src="//cdn-assets-s3.streeteasy.com/assets/desktop-5889dadfb6ca21a47601dcf3361b111f1ab09e71675844748a4f75ceaa01cb48.js"></script>
            <script>
        (function (root) {
          root.SEDeviceType = 'desktop';
        }(window));
      </script>

        <script type="text/javascript">
  jQuery(function() {
    var actionButtons = jQuery('.jsIsSaved, .jsNotSaved'),
        buildingCondition = actionButtons.data('gtm-item-type') === 'Building',
        userAgent = actionButtons.data('user-agent') === false;
    if(buildingCondition || userAgent) {
      actionButtons.removeClass('RealtyActions--small')
    }
    actionButtons.click( function() {
      var $this = jQuery(this);
      if (!$this.find('.jsIconStar').length) {
        if($this.hasClass('jsNotSaved')) {
          $this.removeClass('jsNotSaved');
          $this.addClass('jsIsSaved');
          $this.addClass('RealtyActions--small');
          $this.data('gtm-item-type') === 'Building' ? $this.removeClass('RealtyActions--small') : null;
        } else {
          $this.removeClass('jsIsSaved');
          $this.addClass('jsNotSaved');
          $this.removeClass('RealtyActions--small');
        }
      }
    });
  });
</script>

